{
  "hosting": {
    "public": "build",
    "rewrites": [
      {
        "source": "/manifest.json",
        "destination": "/manifest.json"
      },
      {
        "source": "/robots.txt",
        "destination": "/robots.txt"
      },
      {
        "source": "/**",
        "function": "ssr"
      }
    ]
  },
  "emulators": {
    "storage": {
      "port": 9090
    },
    "functions": {
      "port": 5001
    },
    "database": {
      "port": 9000
    },
    "hosting": {
      "port": 5000
    },
    "ui": {
      "enabled": true
    },
    "auth": {
      "port": 9099
    }
  },
  "functions": {
    "source": "functions"
  },
  "storage": {
    "rules": "storage.rules"
  }
}
